{
  "expo": {
    "name": "EWS Gempa",
    "slug": "eq-ews-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/sounds/ewsalert.ogg"]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.faisal01h.saaewsmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-1024.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.faisal01h.saaewsmobile"
    },
    "web": {
      "favicon": "./assets/images/icon-128.png"
    },
    "description": "Aplikasi Sistem Peringatan Dini Gempa Bumi berdasarkan data dari BMKG."
  }
}
